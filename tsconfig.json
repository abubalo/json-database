{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "allowJs": false,
      "strict": true,
      "noEmitOnError": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noEmit": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["node", "jest"],
      "lib": ["es2018"],
      "outDir": "./",
      "rootDir": "./src",
      "baseUrl": "./"
    },
    "include": ["src/**/*", "package.json", "examples"],
    "exclude": ["node_modules", "test/LuxDB.test.ts", "./dist", ]
  }